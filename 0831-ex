
#include <stdio.h>
/*
內容宣告:
以下程式碼由 李翼全 提供

1. 1+2+3+4+5+......100=?
2. 0+2+4+6+........100=?
3. 1+3+5+7+........+99=?
4. 九九乘法表  
*/

int main()
{
	//1+2+3+...100
	int i;
	int total=0;
	for(i=1;i<=100;i++)
	{
		total=total+i;			  		   	
	}
	printf("total=%d",total);
	
	
	//0+2+4+6+.....100
	int j;
	total=0;
	for(j=0;j<=100;j+=2)
	{
		total=total+i;			  		   	
	}
	printf("total=%d",total);
	
	
	//1+3+5+7+....99
	int k;
	total=0;
	for(k=1;k<=100;k+=2)
	{
		total=total+i;			  		   	
	}
	printf("total=%d",total);
	
	
	//999乘法表
	int m,n;
	for(m=1;m<=9;m++)
	{
		for(n=1;n<=9;n++)
		{
			printf("%d*%d=%d",m,n,m*n);
		}
	} 

	return 0;
}

